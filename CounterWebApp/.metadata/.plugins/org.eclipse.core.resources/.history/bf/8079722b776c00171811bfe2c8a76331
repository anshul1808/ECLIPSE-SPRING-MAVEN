import java.util.*;
public class TestEmp
	{
	public static void main(String[] args)
	{
	List myArrayList = new ArrayList();
	myArrayList.add(1);
	myArrayList.add(5);
	myArrayList.add(3);
	Collections.sort(myArrayList);
	myArrayList.add(2);
	Collections.reverse(myArrayList);
	System.out.print(myArrayList);
	}
}